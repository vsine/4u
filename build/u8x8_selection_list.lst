  72:u8g2_lib/u8x8_selection_list.c ****     if ( u8sl->first_pos > u8sl->current_pos )
  73:u8g2_lib/u8x8_selection_list.c ****       u8sl->first_pos = u8sl->current_pos;
  74:u8g2_lib/u8x8_selection_list.c ****   }
  75:u8g2_lib/u8x8_selection_list.c **** }
